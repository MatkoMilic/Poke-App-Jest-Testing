{
  "name": "pokeappv4jesttesting",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "commitizen": "^4.2.4",
    "enzyme": "^3.11.0",
    "formik": "^2.2.9",
    "jest-react-native": "^18.0.0",
    "merge": "^2.1.1",
    "react": "16.13.1",
    "react-native": "0.63.3",
    "react-native-paper": "^4.10.0",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.9.0",
    "react-query": "^3.32.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@testing-library/react-native": "^8.0.0",
    "@types/enzyme": "^3.10.10",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.34",
    "@types/react-native": "^0.66.4",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^25.1.0",
    "eslint": "^6.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "jest-transform-stub": "^2.0.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "16.13.1",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/testing/fileMock.js",
      "\\.(css|sass)$": "<rootDir>/testing/styleMock.js"
    },
    "transform": {
      ".+\\.(css|scss|png|jpg|svg)$": "jest-transform-stub"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ],
    "setupFiles": [
      "<rootDir>/testing/jest-setup.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
