    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "^16.13.1",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/testing/fileMock.ts",
      "\\.(css|sass)$": "<rootDir>/testing/styleMock.ts"
    },
    "transform": {
      ".+\\.(css|scss|png|jpg|svg)$": "jest-transform-stub"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-native-payfort-sdk|react-native)/"
    ],
    "setupFiles": [
      "<rootDir>/testing/jest-setup.tsx"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
